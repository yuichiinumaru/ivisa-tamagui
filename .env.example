# Copy this file to .env and replace the placeholder values with your own secrets.
# Never commit the populated .env file.

# Google API Key for Gemini (used by both agent and Cognee)
GOOGLE_API_KEY="your-google-api-key"

# Multiple API Keys for Rotation (comma-separated, no spaces)
GENAI_API_KEYS="key01,key02,key03"

# Gemini Model Configuration for Agent
GEMINI_MODEL=gemini-2.5-pro
GEMINI_TEMPERATURE=0.7
GEMINI_TOP_P=0.95
GEMINI_REQUEST_TIMEOUT=900

# Cognee Configuration (Memory System)
LLM_API_KEY="your-cognee-llm-key"

# Cognee LLM Model (internal to Cognee, not the agent)
COGNEE_LLM_MODEL=gemini-2.5-flash
COGNEE_LLM_TEMPERATURE=0.1

# Cognee Embedding Model (for vector embeddings)
COGNEE_EMBEDDING_MODEL=gemini-embedding-001

# Cognee Provider (google, openai, anthropic, etc.)
COGNEE_LLM_PROVIDER=google

# Optional service integrations
E2B_API_KEY="your-e2b-key"
GITHUB_PERSONAL_ACCESS_TOKEN="your-github-token"
SUPABASE_ACCESS_TOKEN="your-supabase-token"
TAVILY_API_KEY="your-tavily-key"
PERPLEXITY_API_KEY="your-perplexity-key"
KAGI_API_KEY="your-kagi-key"
JINA_AI_API_KEY="your-jina-key"
FIRECRAWL_API_KEY="your-firecrawl-key"
